@misc{imbermanDoesMarketValue2012,
  title = {Does the {{Market Value Value}}-{{Added}}? {{Evidence}} from {{Housing Prices After}} a {{Public Release}} of {{School}} and {{Teacher Value}}-{{Added}}},
  author = {Imberman, Scott and Lovenheim, Michael},
  date = {2012-06},
  publisher = {{unpublished paper, Michigan State University}},
  url = {https://www.msu.edu/~imberman/LA_VA_5-17-12.pdf},
  abstract = {Expands on Sandra Black research In this paper, we examine the extent to which people value teacher value-added information by examining how the release of such information is capitalized into home prices. With the exception of Gibbons et. al., to our knowledge this is the first analysis that examines whether housing markets are responsive to school value-added, which is important to understand given the increasing prevalence of value-added models in K-12 education and the increasing availability of these data for parents. Further, our identification strategy avoids known problems with boundary discontinuity papers such as sorting around the boundary that can lead to difficult to interpret local average treatment effects and difficulties with attributing differences in housing prices across boundaries to school quality or other correlates of school quality measures such as demographics. To estimate the impact of school value-added on housing prices, we use a unique and unanticipated release of value-added information for schools and teachers in the Los Angeles Unified School District (LAUSD). In August, 2010 the Los Angeles Times (LA Times) released valueadded estimates for 6,000 teachers in over 470 elementary schools in LAUSD on their website, which was easily accessed to the public and searchable by school and teacher.},
  file = {/Users/jdoughe2/Zotero/storage/AHVPWBP7/LA_VA_5-17-12.pdf}
}

