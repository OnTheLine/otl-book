@book{achievehartfordCohortReportHow2015,
  title = {Cohort {{Report}}: {{How Fast}} Are {{Hartford Students Progressing}}?},
  author = {Achieve Hartford},
  date = {2015-02},
  location = {{Hartford, CT}},
  url = {http://smarterhartford.org/reports/cohort/},
  abstract = {From AH Education Matters 19 Feb 2015: "The academic progress of Hartford students from Grade 3 to Grade 8 shows growth – especially at Grade 6 – before a drop-off in Grades 7 and 8, according to a new mini-report issued on Achieve Hartford's newly launched SMARTERHartford.org website. But that’s not all it shows. Looking at the results from three cohorts of students on the CT Mastery Test (CMT), the mini-report examined data for Hartford children who began taking the CMT in 2005-2006, 2006-2007, and 2007- 2008, respectively, to see how much they progressed year over year until eighth grade. The three groups were not matched cohorts – that is, attrition data were not available to account for migration of students from school to school or new arrivals at schools during the years examined. Yet, at a high level, the data for these last three cohorts to take the CMT, before its discontinuation in 2013, do show modest increases through Grade 5 and larger increases at Grade 6, before a drop-off at Grades 7 and 8. While the academic velocity of individual Hartford schools has wide variability, magnet schools typically see more growth over the years and have higher levels of performance than do neighborhood schools – but not in all cases, especially when you break out the performance of only Hartford students at magnet schools. The drop-off in performance from Grade 6 to Grade 8 is a major concern, as it negates much of what students were able to gain from Grade 3 to Grade 6 – and makes it nearly impossible to close the achievement gap with the State average by high school graduation. It also comes at a crucial time, just before the transition to high school. The detailed results in the full mini-report are available here. Academic velocity findings at each individual school are fascinating – but mostly as they relate to the variability of each school’s performance, given that the cohort data we have do not account for migration of students from one school to another."},
  file = {/Users/jdoughe2/Zotero/storage/FBTQU7IS/cohort-analysis-final.pdf}
}

@misc{achievehartfordGettingOptionsOpportunities2014,
  title = {Getting the {{Options}} and {{Opportunities Right}}: {{How Listening}}, {{Learning}}, and {{Revising Can Increase}} the {{Impact}} of {{Sheff}}},
  author = {Achieve Hartford},
  date = {2014-12-17},
  url = {http://achieve.s464.sureserver.com/sites/default/files/civicrm/persist/contribute/files/Getting%20the%20Options%20Right.pdf},
  abstract = {"As Hartford has steadily increased the percentage of its students enrolled in racially and socio-economically integrated schools, the gap between high-quality and chronically low-performing schools has emerged as a defining issue for the city and regional futures. To address it, we believe, stipulations in the Sheff v. O’Neill case over quality integrated education in Greater Hartford ought to be re-adjusted to better address verifiable needs among the city’s students and families. One result would be to return the perception of Sheff among Hartford families to where it started: a landmark case and catalyst for educational quality in Hartford. This is important. In recent years, last-minute magnet school designations have sprung suddenly upon families and significantly undermined grassroots support. With all of this in mind, Achieve Hartford! recommends eight improvements for the 2014-15 Sheff negotiations between the State and the plaintiffs. In the past, the negotiations have both expanded – and led to unintended consequences that curtail – parent options. To get those options and opportunities right, a fine-grain reconsideration is timely. Here are our eight recommendations:"},
  file = {/Users/jdoughe2/Zotero/storage/3PI7PHX7/AH-SheffGettingTheOptionsRight2014-12-17.pdf}
}

