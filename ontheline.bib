@unpublished{bowmanEducativeNeighborhoodsTemporary2015,
  title = {Educative {{Neighborhoods}} or {{Temporary Sites}}? {{The Lanham Act}}, {{Race}}, and the {{Political Economy}} of {{Permanence}} in {{Seattle}}, 1940-1954},
  author = {Bowman, Michael},
  date = {2015-11},
  abstract = {This paper is part of a policy history that seeks to briefly tell the story of the Lanham Act and its local implementation in one of the nation’s most congested war production centers. I make two primary arguments. First, following historian Kristin Szylvian, I argue that the Lanham Act should be divided into three distinct periods: a period between 1940 and mid-1942 when federal funds went toward the construction of what I call ‘educative neighborhoods’; a period between mid-1942 and 1945 when Lanham funds went toward the construction of temporary sites; and a post-war period between 1945 and 1954 when war housing and school facilities were either shuttered, sold, or transferred to local public authorities. Second, in the case of Seattle, I argue that the industrial recruitment of Southern black labor to work at Boeing and Seattle shipyards beginning in the summer of 1942 corresponded with the shift from permanent to temporary housing construction. This shift would set the stage for post-war civil rights activism as temporary housing closed and a racially restricted housing market re-concentrated African American war workers, veterans, and their families.},
  eventtitle = {History of {{Education Society}}},
  file = {/Users/jdoughe2/Zotero/storage/CVKFSCND/HES2015_final.docx},
  venue = {{St. Louis, MO}}
}

