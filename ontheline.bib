@article{heathcottStrangeCareerPublic2012,
  title = {The {{Strange Career}} of {{Public Housing}}},
  author = {Heathcott, Joseph},
  date = {2012},
  journaltitle = {Journal of the American Planning Association},
  volume = {78},
