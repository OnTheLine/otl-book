  url = {https://digitalcommons.law.yale.edu/student_legal_history_papers/33},
  abstract = {Equal housing opportunity for all people regardless of race has been the law in the United Statesfor almost forty years. Nevertheless, racial residential segregation persists. This paper examines the extent to which fair housing laws and institutions have met their goal of promoting racial residential integration in Greater New Haven, Connecticut. For the purposes of this paper, Greater New Haven is defined as the fifteen towns and cities that comprise the South Central Connecticut Regional Council of Governments,[1] which includes Bethany, Branford, East Haven, Guilford, Hamden, Madison, Meriden, Milford, New Haven, North Branford, North Haven, Orange, Wallingford, West Haven, and Woodbridge.[2] Part II examines racial demographic change in Greater New Haven from 1970 to 2000. In this section, I highlight the region’s population growth and increased racial diversity during this period, and analyze the extent of neighborhood racial integration in the area. I also present the results of a block-level analysis of the racially integrated neighborhoods inNew Haven,West Haven,Hamden, andMeridento determine the extent of racial mixing at a micro level. Finally, I evaluate the stability of racially integrated neighborhoods over the course of the thirty-year period.},
  annotation = {http://web.archive.org/web/20200518144638/https://digitalcommons.law.yale.edu/student\_legal\_history\_papers/33/},
  file = {/Users/jdoughe2/Zotero/storage/U9THNR2Z/Terry - Racial Residential Integration in Greater New Have.pdf;/Users/jdoughe2/Zotero/storage/5E97RRAB/33.html}
}

@article{tevisJewsNotAdmitted2021,
  title = {“{{Jews Not Admitted}}”: {{Anti}}-{{Semitism}}, {{Civil Rights}}, and {{Public Accommodation Laws}}},
  shorttitle = {“{{Jews Not Admitted}}”},
  author = {Tevis, Britt P},
  date = {2021-03-01},
  journaltitle = {Journal of American History},
  shortjournal = {Journal of American History},
  volume = {107},
  pages = {847--870},
  issn = {0021-8723},
  doi = {10.1093/jahist/jaaa461},
  url = {https://doi.org/10.1093/jahist/jaaa461},
  urldate = {2021-03-04},
  abstract = {"This article illuminates how the Seligman affair transformed from a symbol of the precarity of American Jews' civil rights to scholars' primary example of social anti-Semitism. In tracing this shift, I show that the Seligman affair exemplifies what I call legally sanctioned anti-Semitism: anti-Jewish discrimination enabled by state and federal legislation or permitted by the judiciary. As a classification, legally sanctioned anti-Semitism applies to assorted incidents that scholars have understood as disparate and anomalous. For example, it characterizes instances of express discrimination such as when New Hampshire's constitution required state officeholders to be Protestant (a prerequisite that lasted until 1877). Secondly, it describes seemingly neutral legislation that disproportionately disadvantaged Jews because of their Jewish practices or identities. For example, Sunday closing laws that restrained Jews' power to trade on the Christian Sabbath harmed Jewish workers' capacity to earn money because Jews also refrained from trade on Saturdays in recognition of the Jewish Sabbath. Third, legally sanctioned anti-Semitism characterizes lawmakers' failure to ensure Jews' rights by neglecting to pass protective legislation. Quotas limiting the number of Jewish students at private universities may have been inspired by anti-Jewish animus, but the legality of such policies depended on the absence of legislation prohibiting such quotas. Fourth, legally sanctioned anti-Semitism highlights circumstances in which parties knowingly violated laws based on the judiciary's tacit assurance of legality. Hilton, for example, conceded that he probably had violated the law when he excluded Seligman, anticipating that he would not face legal repercussions. Finally, legally sanctioned anti-Semitism reflects the judiciary's endorsement of anti-Jewish discrimination through “rights-denying court decisions” in which judges struck down protective legislation, thereby making policy choices about Americans' freedoms. The 1883 decision on the group of five cases that constituted the Civil Rights Cases and the 1896 Supreme Court decision in Plessy v. Ferguson, for example, legally permitted white business owners to exclude Jews (and, of course, Black Americans) from places of public accommodation. Ultimately, these episodes are connected by the fact that, in each case, non-Jews employed the legal system to institutionalize anti-Jewish bias"},
  file = {/Users/jdoughe2/Zotero/storage/7KJLQCFU/Tevis - 2021 - “Jews Not Admitted” Anti-Semitism, Civil Rights, .pdf;/Users/jdoughe2/Zotero/storage/ABCXIYNL/6157131.html},
  number = {4}
}

