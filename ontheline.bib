@article{highsmithSegregationSplittingSegregation2015,
  title = {Segregation as {{Splitting}}, {{Segregation}} as {{Joining}}: {{Schools}}, {{Housing}}, and the {{Many Modes}} of {{Jim Crow}}},
  shorttitle = {Segregation as {{Splitting}}, {{Segregation}} as {{Joining}}},
  author = {Highsmith, Andrew R. and Erickson, Ansley T.},
