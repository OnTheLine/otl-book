  date = {2009},
  journaltitle = {Peabody Journal of Education},
  volume = {84},
  pages = {130--149},
  url = {http://dx.doi.org/10.1080/01619560902810104},
  abstract = {"Using student-level data from Durham, North Carolina, we examine the potential impact of school choice programs on the peer environments of students who remain in their geographically assigned schools. We examine whether the likelihood of opting out of one's geographically assigned school differs across groups and compare the actual peer composition in neighborhood schools to what the peer composition in those schools would be under a counterfactual scenario in which all students attend their geographically assigned schools. We find that many advantaged students have used school choice programs in Durham to opt out of assigned schools with concentrations of disadvantaged students and to attend schools with higher achieving students. Comparisons of actual peer compositions with the counterfactual scenario indicate only small differences in peer composition for nonchoosers on average. More substantial differences in peer environment emerge, however, for students in schools with concentrations of disadvantaged students and schools located near choice schools attractive to high achievers. The results suggest that expansions of parental choice may have significant adverse effects on the peer environments of a particularly vulnerable group of students."},
  file = {/Users/jdoughe2/Zotero/storage/T7VIRBGB/Bifulco et al 2009 8420.pdf;/Users/jdoughe2/Zotero/storage/NKHDVHET/Bifulco - 2009 - The Effects of Public School Choice on Those Left .html},
  keywords = {CSS,GIS,School choice},
  number = {2}
}

@inproceedings{bifulcoMarketMattersParental2010,
  title = {Market {{Matters}}: {{Parental Demand}} in an {{Interdistrict Choice Program}}},
  booktitle = {American {{Educational Research Association}}},
  author = {Bifulco, Robert and Cobb, Casey D and Bell, Courtney and Wanzer, Jesse},
  date = {2010},
  abstract = {Investigates parental demand and participation in Open Choice in Hartford from 2002 to 2006 (and acknowledges that policies have shifted since this time) While Frankenberg 2007 argued that the demand for Open Choice seats far exceeds the supply, this needs some qualification. First, due to the high rate of students who did not accept a lottery invitation to enroll in Open Choice, comparing only applicants to open seats overstates the number. Second, Open Choice participation varies widely across grade levels, with virtually all PreK-2 students with reasonable lottery numbers to be placed, but less so in higher grade levels. p2-3 See also the "sibling privilege" in Open Choice: 11 percent of applicants had sibling currently enrolled, and received lottery preference (which has the unintended effect of perpetuating racial patterns). **Spatial that authors may not have explored: Are urban families more likely to decline Open Choice placements as the location of the suburban school increases in distance from Hartford? **LOOK TO SEE HOW CREC vs RSCO decline data is collected. Does it include name of placement district/school? Conclusion: "In summary, Black students in Hartford are both more likely to apply to Open Choice and more likely to accept an offer to participate than are Hispanic students. The result is substantially higher participation rates among Black students than Hispanic students. Males and females are equally likely to apply to Open Choice, but females are more likely to accept an offer to participate, leading to slightly higher participation rates among females. Finally, we found some differences in participation by geography. Particularly, a relatively high percentage of students from school attendance zones located in the northern sections of Hartford apply to and participate in Open Choice. The concentration of Open Choice students from these attendance zones can be observed despite the fact that these areas are not located closer to districts that offer Open Choice seats. Efforts to expand Open Choice and to ensure equal access to all Hartford students should begin with investigations of why Hispanic students, males, and students from the southern sections of the city are less likely to participate." Note that although authors claim that "student race is not used in determining admissions" (p. 1), the CT statutes restrict the degree of participation by race and residence (see my Sheff chapter).},
  file = {/Users/jdoughe2/Zotero/storage/U37TSUPB/Bifulco et al 2010.pdf},
  keywords = {CSS,GIS,Hartford,School choice}
}

@article{bifulcoPublicSchoolChoice2008,
  title = {Public {{School Choice}} and {{Integration}}: {{Evidence}} from {{Durham}}, {{North Carolina}}},
  author = {Bifulco, Robert and Ladd, Helen F and Ross, Stephen},
  date = {2008},
  journaltitle = {Center for Policy Research, Syracuse University},
  volume = {Working Paper No. 109},
  abstract = {abs: "Using evidence from Durham, North Carolina, we examine the impact of school choice programs on racial and class-based segregation across schools. Theoretical considerations suggest that how choice programs affect segregation will depend not only on the family preferences emphasized in the sociology literature but also on the linkages between student composition, school quality and student achievement emphasized in the economics literature. Reasonable assumptions about the distribution of preferences over race, class, and school characteristics suggest that the segregating choices of students from advantaged backgrounds are likely to outweigh any integrating choices by disadvantaged students. The results of our empirical analysis are consistent with these theoretical considerations. Using information on the actual schools students attend and on the schools in their assigned attendance zones, we find that schools in Durham are more segregated by race and class as a result of school choice programs than they would be if all students attended their geographically assigned schools. In addition, we find that the effects of choice on segregation by class are larger than the effects on segregation by race."},
  file = {/Users/jdoughe2/Zotero/storage/3HF8QXTM/Bifulco et al 2008 5909.pdf},
  keywords = {CSS,School choice}
}

