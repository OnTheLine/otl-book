@article{ayersFredTrumpKu2021,
  title = {Fred {{Trump}}, the {{Ku Klux Klan}} and {{Grassroots Redlining}} in {{Interwar America}}},
  author = {Ayers, Oliver},
  date = {2021-01-01},
  journaltitle = {Journal of Urban History},
  shortjournal = {Journal of Urban History},
