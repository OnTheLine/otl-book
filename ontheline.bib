@thesis{hanleyDesegregatingHartfordCan2017,
  title = {Desegregating {{Hartford}}: {{Can}} the {{Sheff}} v {{O}}'{{Neill Efforts Survive}} the {{Challenges Ahead}}?},
  author = {Hanley, Johanna},
  date = {2017},
  institution = {{University of Georgia}},
  abstract = {This paper examines the implementation of school desegregation reform and policy efforts in the greater Hartford region of Connecticut, with a focus on the years 2008-2015. ~These efforts have been the result of the 1996 Connecticut Supreme Court decision in Sheff v. O’Neill, which mandated Connecticut to desegregate the racially isolated schools in Hartford, its capital.  Today, the two main components of compliance with Sheff v. O’Neill are: (1) an interdistrict magnet school system, and (2) a voluntary two-way busing program called Open Choice. ~These programs, operated by the Capital Region Education Council (CREC), have achieved substantial progress toward their goal of school integration in the Hartford region, but many Hartford students remain underserved in racially isolated schools.  This paper explores and analyzes these Sheff-related efforts using an educational change framework; they are now in the continuation phase, but their future remains unclear.  The Hartford school system and interdistrict reforms face numerous challenges ahead, but their successes and failures should be learned from.  Despite desegregation being court mandated, Hartford is a model for integration reforms and interdistrict cooperation, and this paper seeks to shed light on the successful aspects of these efforts and challenges for the future.},
  file = {/Users/jdoughe2/Zotero/storage/BJVUR8I4/Hanley_EDAP 9010_Final Paper.docx},
  type = {Unpublished EDAP 9010 paper}
}

@thesis{hanleyWeJustDon2017,
  title = {"{{We Just Don}}'t {{Want To Do It}}": {{The Legacy}} of {{Sheff}} v {{O}}'{{Neill}} in the {{Shadow}} of {{Brown}} v {{Board}} of {{Education}}},
  author = {Hanley, Johanna},
  date = {2017},
  institution = {{University of Georgia}},
  abstract = {The Sheff v. O’Neill school desegregation case, originally filed in 1989, created an integration movement in the Hartford region that has lasted nearly 30 years.  Despite successes and national recognition, there appears to be a loss of political will and questioning of the merits of integration for the region’s students that has caused efforts to stall.  The Sheff movement has not been forgotten, but to a certain extent misunderstood and bypassed.  Similar to Brown v. Board of Education, the legacy of Sheff is troubled—causing numerous stakeholders to question whether benefits have outweighed the costs, particularly for students of color.  The Sheff case and its outcomes are explored to the present day, discussing future implications for students of color, a strategy of regionalism, and considerations of where the movement and public education in the region goes from here.},
  file = {/Users/jdoughe2/Zotero/storage/HTMPI2VS/Hanley_EDAP8130_Final Paper.docx},
  type = {Unpublished EDAP 8130 paper}
}

