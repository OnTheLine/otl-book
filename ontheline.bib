@online{edbuildDividingLinesSchool2015,
  title = {Dividing {{Lines}}: {{School District Borders}} in the {{United States}}},
  author = {{EdBuild}},
  date = {2015-07},
  url = {http://maps.edbuild.org/DividingLines.html},
  urldate = {2015-07-21},
  abstract = {Our current school funding system often bolsters school district boundaries between rich and poor, holding resources in wealthy communities and keeping low-income students from accessing broader opportunities. In June, The Supreme Court decided Texas v. Inclusive Communities Project, affirming that no government policy, even one without discriminatory intent, may create “artificial, arbitrary, and unnecessary barriers” for minority individuals and families seeking quality housing in a good neighborhood. However, one kind of artificial, arbitrary, and unnecessary barrier continues to separate families—often poor and minority families—from places of opportunity: school-district boundaries. In the United States, the public school a student attends is still primarily determined by where their family lives. Most children are enrolled in district schools that receive, on average, nearly half of their funding through local property taxes. This system ties school budgets to the value of local property wealth and incentivizes boundaries between upper- and lower-income communities. Intentional or not, these invisible walls often concentrate education dollars within affluent school districts, and ensure that low-income students are kept on the outside. This map, which shows student poverty levels within the more than 13,000 school districts in the United States, begins to paint a picture of this phenomenon: school districts drawn in odd shapes and sizes, many sitting next to—or even inside of—districts of wildly different means},
  file = {/Users/jdoughe2/Zotero/storage/G8UGIPEF/DividingLines.html},
  organization = {{EdBuild}}
}

