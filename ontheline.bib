@misc{rappaAuthorityLocalLegislative2001,
  title = {The {{Authority}} of {{Local Legislative Bodies}} to {{Impose Land Use Moratoria}}: 2001-{{R}}-0782},
  author = {Rappa, John G.},
  date = {2001-10-17},
  publisher = {{Office of Legislative Research, State of Connecticut}},
  url = {https://www.cga.ct.gov/2001/rpt/2001-R-0782.htm},
  urldate = {2021-04-19},
  abstract = {The zoning statutes set no conditions under which legislative bodies can exercise zoning powers, including imposing moratoria. In 1956, the Connecticut Supreme Court interpreted these statutes to mean towns can exercise zoning powers only through their zoning commissions (Olson v. Avon, 143 Conn. 448). (The 1984 Bernhard decision (194 Conn. 152) upheld the right of these commissions to impose moratoria.) In 1939, Avon adopted the zoning statutes and created a zoning commission. In 1947, the legislature amended those statutes, requiring commissions to hold public hearings on proposed zone changes and to submit them to the legislative body for approval if a majority of the people who attended the hearing opposed the changes. Avon immediately adopted this change, but a year later adopted a more restrictive regulation. It prohibited the commission from amending, changing, or repealing a regulation without the legislative body's approval. In 1951, the legislature repealed the statute conditionally requiring legislative body approval. But Avon kept its restrictive regulation. In 1955, the commission submitted a proposed zone change it had unanimously approved to the legislative body, which subsequently rejected it. The applicant appealed, claiming the regulation was not valid, and the Supreme Court agreed (Olson). The court cited an earlier decision in which it held the 1925 zoning statute left “no doubt that it was the intent of the General Assembly to vest in a zoning commission the sole authority to make, amend or repeal regulations; and for that purpose the zoning commission became the legislative agency of the municipality. To admit that a town meeting could amend or repeal regulations duly made by the commission would be to recognize in it a power directly at variance with the legislative intent” (State ex. rel. Bezzini v. Hines, 133 Conn. 592 (1947)).},
  file = {/Users/jdoughe2/Zotero/storage/C2FD3MMS/2001-R-0782.html}
}

