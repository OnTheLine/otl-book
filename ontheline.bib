@article{adelmanRolesRaceClass2005,
  title = {The {{Roles}} of {{Race}}, {{Class}}, and {{Residential Preferences}} in the {{Neighborhood Racial Composition}} of {{Middle}}-{{Class Blacks}} and {{Whites}}*},
  author = {Adelman, Robert M.},
  date = {2005-03-01},
  journaltitle = {Social Science Quarterly},
