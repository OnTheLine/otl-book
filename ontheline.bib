@article{hillierResidentialSecurityMaps2005,
  title = {Residential {{Security Maps}} and {{Neighborhood Appraisals}}: {{The Home Owners}}' {{Loan Corporation}} and the {{Case}} of {{Philadelphia}}},
  author = {Hillier, Amy},
  date = {2005},
  journaltitle = {Social Science History},
  volume = {29},
  pages = {207--233},
  url = {https://doi.org/10.1017/S014555320001292X},
  abstract = {At the request of the Home Loan Bank Board, the Home Owners’ Loan Corporation (HOLC) created color-coded maps for cities across the country between 1935 and 1940 that indicated risk levels for long-term real estate investment. Involvement in this City Survey Program marked a departure from the original mission of HOLC to provide new mortgages on an emergency basis to homeowners at risk of losing their homes dur- ing the Depression. This article considers why HOLC made these maps, how HOLC created them, and what the basis was for the grades on the maps. Geographic infor- mation systems and spatial regression models are used to show that racial composition was a significant predictor of map grades, controlling for housing characteristics.},
  file = {/Users/jdoughe2/Zotero/storage/7QNC5KFV/Hillier 2005.pdf},
  keywords = {CSS,GIS},
  number = {2}
}

@article{hillierSearchingRedLines2005,
  title = {Searching for {{Red Lines}}: {{Spatial Analysis}} of {{Lending Patterns}} in {{Philadelphia}}, 1940-1960},
  author = {Hillier, Amy},
  date = {2005},
  journaltitle = {Pennsylvania History},
  volume = {72},
  pages = {25--47},
  url = {https://jstor.org/stable/27778657},
  abstract = {PDF redlining: "commonly understood as the systematic denial of mortgages or insurance based on the racial composition of the applicant's neighbrohood rather than the credit-worthiness of teh borrower" p25 author argues that "hitorians have not given as much attention to mortgage redlining as other forms of housing discrimination" p25, such as Sugrue, Meyer (blacks harassed when moving into white neighborhoods); Helper, Mohl (systematic bias in real estate and appraisal firms) "Because redlining involves discrimination baed on the characteristics of the neighborhood rather than an individual, it requires a different form of evidence to demonstrate if, and how, it happened." p26 HOLC created maps for many cities 1936-40 to assess risk levels vs FHA subsidized new construction in predominantly white suburban developments, by extending insurance and eliminating risk Author: HOLC maps "reflected widespread acceptance of race-based neighborhood appraisal standards" but "were not distributed widely enough, or kept sufficiently up-to-date to ahve been used by lenders to decide where to make loans" p26 However, while FHA's race-based standards were more widely distributed through its Underwriting Manual, "the lack of small-area data about where FHA insured loans has made it difficult to assess FHA's impact on lending at anything below the country level" p26 author's approach: rather than analyzing impact of HOLC redlining maps, "this methodology focuses on mapping address-level lending data and then searcing for red lines -- areas underserved or served differently by mortgage lenders," applied to random samples of Philadelphia mort data from 1940-60 Ken Jackson documented FHA's "strong suburban bias" with data for 5 metro areas from 1934-1960. In St. Louis, the city received 1/5 FHA mortgages as surrounding county; during same period, less than 2\% FHA loans made to blacks; deliberate discrimination, not unintended, p29. "But detailed case studies documenting lending patterns at anything below the county level have been absent." p31 source challenge: lack of aggregate mortgage data before passage of Home Mortgage Disclosure Act (1975) solution: three locally constructed data sets 1) sample of property transactions from Phil Realty Directory, 1940-60 2) City of Phil archived mortgage records 3) random sample of sheriff's sales Distribution of mortgages: -- pattern is "better explained by the distribution of owner-occupied housing than the presence of blacks or new housing" p34 Distribution of interest rates: "intereste rates tended to be higher in a ring around the central part of the city [with higher concentration of blacks] and lower in the outlying areas" p35 Distribution by lender type: certain types of lenders were more likely to make loans to central-city black neighborhoods than other types of lenders Discussion: homes in central core (generally older homes and black concentrations) "did have access to mortgage credit, but they had fewer choices in the type of lende and paid higher interest rates" p42 But lack of address-level FHA data "makes it difficult to prove that none, or few" of central-area homes received FHA-insured mortgages, though this is most likely true p42 "Does discrimination account for the distinct lending patterns demosntrated in this study, and do those patterns account for the ctoinued problems with segregation, poverty, housing disrepair, and population loss those areas face?" p43 -- despite rationalist argument of higher cost of lending in certain geographic areas, and cause \& effect issues about higher interest rates vs foreclosures, the combined effect of "other forms of discrimiation" enas "the answer is probably 'yes'." p43 Conclusion: "limited access to mortgage credit contributed to [inequality]... "along with race-restrictive covenants, concentrated public housing, failed urban renewal efforts, and federally subsidized suburbanization" p44},
  file = {/Users/jdoughe2/Zotero/storage/6QP4WQMT/Hillier 2005 3361.pdf},
  keywords = {CSS,GIS},
  number = {1}
}

@article{hillierSpatialAnalysisHistorical2003,
  title = {Spatial {{Analysis}} of {{Historical Redlining}}: {{A Methodological Exploration}}},
  author = {Hillier, Amy},
  date = {2003},
  journaltitle = {Journal of Housing Research},
  volume = {14},
  pages = {137--167},
  url = {http://jstor.org/stable/44944777},
  abstract = {Despite widespread belief that redlining contributed to disinvestment in cities, there has been little empirical analysis of historical lending patterns. The lack of appropriate data and clear definitions of redlining has contributed to this void. This article reviews definitions and methods that have emerged from research on lending in recent years and considers how they can be applied to research on historical redlining. Address-level mortgage data from Philadelphia from the 1940s are analyzed using spatial regression, "hot spot" analysis, and surface interpolation. Employing multiple definitions of redlining that focus on process and outcome, as well as spatial and statistical relationships in lending, the analyses result in a series of map layers that indicate where redlining may have occurred. In addition to providing some evidence of lending discrimination, this article promotes an explicitly spatial view of redlining that has conceptual and methodological implications for research on contemporary and historical redlining.},
  file = {/Users/jdoughe2/Zotero/storage/B9K3ZCBX/Hillier 2003 1734.pdf},
  keywords = {CSS,GIS},
  number = {1}
}

