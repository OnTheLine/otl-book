@incollection{lubienskiPoliticsParentalChoice2008,
  title = {The {{Politics}} of {{Parental Choice}}: {{Theory}} and {{Evidence}} on {{Quality Information}}},
  booktitle = {School Choice Policies and Outcomes : Empirical and Philosophical Perspectives},
  author = {Lubienski, Christopher},
  editor = {Feinberg, Walter and Lubienski, Christopher},
  date = {2008},
  pages = {99--119},
  publisher = {{SUNY Press}},
  location = {{Albany}},
  url = {https://play.google.com/books/reader?id=7I_xn57WoiAC&printsec=frontcover},
  abstract = {see also full volume obtained digital draft from author ABSTRACT: Although there are many, often competing, justifications for school choice, almost all are premised on the notion that parents are to be empowered to select the best schools for their children. While this assumption derives from economic theory, it too often devolves into an ideological debate regarding whether or not parents are able to choose a school in their child’s best interests. This analysis examines two types of arguments that parents are necessarily the most effective advocates for their children’s educational interests: first, a philosophical contention that parents are necessarily best positioned to exercise primary responsibility over their children’s education; and secondly, an empirical claim that parents choose schools based on evidence of academic quality. The analysis finds each of these claims to be problematic in light of evidence on the dynamics of school choice in the real-world context. Instead of debating whether or not parents can choose the best schools, I argue that the appropriate question is whether the requisite knowledge or information is widely available and equitably distributed through mechanisms of choice and competition. To that end, I introduce a framework for considering types of information that could be employed in selecting a school. A short empirical exercise demonstrates that more useful types of information are least likely to be available to parents.},
  file = {/Users/jdoughe2/Zotero/storage/MGF7G7IC/LubienskiPoliticsParentalChoice2008.pdf},
  isbn = {9780791475713 (hardcover : alk. paper)},
  keywords = {CSS,Ed Policy,School choice}
}

