@report{achievehartfordReportStudentCohort2012,
  title = {Report on {{Student Cohort Growth}} in {{Hartford}}: {{A Supplement}} to {{Achieve Hartford}}'s 2012 {{CMT}} and {{CAPT Scores Summary Report}}},
  author = {Achieve Hartford},
  date = {2012-10-11},
  location = {{Hartford, CT}},
  url = {http://achievehartford.org/upload/files/10-11-2012Cohort%20Report%20Final.pdf},
  abstract = {The most recent results of the Connecticut Mastery Test (CMT) and the Connecticut Academic Performance Test (CAPT)were released in July 2012. These standardized tests are administered on an annual basis to students in grades 3 through 8 (CMT) and in grade 10 (CAPT) to assess how well students, schools, and districts in Connecticut are meeting the standards of achievement set by the State Department of Education.In August, Achieve Hartford! released an analysis of the district’s performance on these tests, specifically examining the impact the district’s different models of schools havehad on student achievement.This new report looks at the student results from a different angle: the student cohort. Whereas the current method of reporting CMT and CAPT results provides a “snapshot” of student achievement in a given year, the cohort approach allows us to track the performance of a group of students as they progress from late elementary school into high school. With this analysis, we can address the important question of whetherstudents are building on gains in learning from prior years.The purpose of this report, however, is not to provide a comprehensive evaluation of the district or particular schools; instead, this report seeks to provide (1) a detailed explanation of a relatively new method of measuring student and school performance, (2) demonstrate the utility of this new method, and (3) highlight just a couple areas of success and concern for the benefit of not only the school district, but for the greater community as well},
  file = {/Users/jdoughe2/Zotero/storage/PX5AITCD/10-11-2012Cohort Report Final.pdf}
}

