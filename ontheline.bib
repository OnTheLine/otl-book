@article{whittemoreHowFederalGovernment2013,
  title = {How the {{Federal Government Zoned America}}: {{The Federal Housing Administration}} and {{Zoning}}},
  author = {Whittemore, Andrew H.},
  date = {2013-07-01},
  journaltitle = {Journal of Urban History},
  shortjournal = {Journal of Urban History},
  volume = {39},
  pages = {620--642},
  issn = {0096-1442, 1552-6771},
  doi = {10.1177/0096144212470245},
  url = {http://juh.sagepub.com/content/39/4/620},
  urldate = {2013-06-15},
  abstract = {Zoning has had an unmistakable impact on the American built environment. It is very significant then that federal policy has had an impact on zoning. The Federal Housing Administration (FHA) has been a significant instrument for this policy. It has biased zoning toward the production of suburban development, an urban form marked by sprawling forms and the strict segregation of uses. While the FHA’s impact on racial segregation and the marketplace have been well researched, the FHA’s impact on zoning, and therefore on the vital components of urban form including the arrangement of uses and densities, has not been. Using a case study of the Los Angeles experience, this paper argues that the FHA, by offering a financing option for products much in demand, persuaded cities to adjust their zoning according to federal standards. The FHA provided a convincing case that zoning for lower-density residential uses and preserving them was the best way to accommodate the huge market of hopeful homeowners eager to acquire FHA-backed mortgages.},
  file = {/Users/jdoughe2/Zotero/storage/FSGBMDEK/Whittemore - 2013 - How the Federal Government Zoned America The Feder.pdf;/Users/jdoughe2/Zotero/storage/NVEK7TQ8/620.html},
